{
  "name": "acm-template-pro-generator",
  "description": "ACM 示例模板生成器",
  "basePackage": "xyz.linyh2",
  "version": "1.0",
  "author": "linyh",
  "createTime": "2023-12-29",
  "fileConfig": {
    "inputRootPath": "F:/AllIdeaProject/esay-code-generator/easy-code-generator-maker",
    "outputRootPath": "generated",
    "type": "dir",
    "files": [
      {
        "inputPath": "src/main/resources/templates/AcmMainTemplate.java.ftl",
        "outputPath": "src/xyz/linyh2/acm/MainTemplate.java",
        "type": "file",
        "generateType": "dynamic"
      },
      {
        "inputPath": ".gitignore",
        "outputPath": ".gitignore",
        "type": "file",
        "generateType": "static"
      },
      {
        "inputPath": "README.md",
        "outputPath": "README.md",
        "type": "file",
        "generateType": "static"
      }
    ]
  },
  "modelConfig": {
    "models": [
      {
        "fieldName": "loop",
        "type": "boolean",
        "description": "是否生成循环",
        "defaultValue": false,
        "abbr": "f"
      },
      {
        "fieldName": "author",
        "type": "String",
        "description": "作者注释",
        "defaultValue": "\"linyh\"",
        "abbr": "a"
      },
      {
        "fieldName": "outputText",
        "type": "String",
        "description": "输出信息",
        "defaultValue": "\"sum =\" ",
        "abbr": "o"
      }
    ]
  }
}