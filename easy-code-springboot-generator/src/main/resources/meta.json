    "outputRootPath": "springboot-generated",
    "type": "dir",
    "files": [
      {
        "inputPath": "src/main/resources/templates/AcmMainTemplate.java.ftl",
        "outputPath": "src/xyz/linyh/acm/MainTemplate.java",
        "type": "file",
        "generateType": "dynamic"
      },
      {
        "inputPath": "src/main/resources/templates/AcmMainTemplate.java.ftl",
        "outputPath": "src/xyz/linyh/acm/MainTemplate.java",
        "type": "group",
        "condition": "needGit",
        "files": [
          {
            "inputPath": ".gitignore",
            "outputPath": ".gitignore",
            "generateType": "static"
          },
          {
            "inputPath": "README.md",
            "outputPath": "README.md",
            "type": "file",
            "generateType": "static"
          }
        ]
      },
      {
        "inputPath": ".gitignore",
        "outputPath": ".gitignore",
        "type": "file",
        "condition": "needGit",
        "generateType": "static"
      },
      {
        "inputPath": "abc",
        "outputPath": "abc",
        "type": "file",
        "generateType": "static"
      },
      {
        "inputPath": "README.md",
        "outputPath": "README.md",
        "type": "file",
        "generateType": "static"
      }
    ]
  },
  "modelConfig": {
    "models": [
